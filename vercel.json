{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!images|vendors|videos|hero|favicon|robots\\.txt|manifest\\.json|.*\\.json|.*\\.(png|jpg|jpeg|gif|svg|webp|avif|ico|css|js|mp4|webm)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(images|vendors|videos)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
