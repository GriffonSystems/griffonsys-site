{
  "rewrites": [
    {
      "source": "/((?!images|videos|vendors|hero|favicon|robots\\.txt|manifest\\.json|.*\\.json).*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/hero/index.json",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
