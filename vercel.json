{
  "rewrites": [
    {
      "source": "/((?!images|vendors|videos|hero|favicon|robots\\.txt|manifest\\.json|.*\\.json).*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/hero/index.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/(images|vendors|videos)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/videos/(.*)\\.mp4",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/videos/(.*)\\.webm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/webm"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
